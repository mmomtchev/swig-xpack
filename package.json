{
  "name": "@mmomtchev/swig-xpack",
  "version": "5.0.11-1",
  "description": "SWIG JSE xPack",
  "scripts": {
    "postversion": "git push && git push --tags && node ./publish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmomtchev/swig-xpack.git"
  },
  "keywords": [
    "swig",
    "swig-jse",
    "wasm",
    "node-api",
    "xpack"
  ],
  "author": "Momtchil Momtchev <momtchil@momtchev.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmomtchev/swig-xpack/issues"
  },
  "homepage": "https://github.com/mmomtchev/swig-xpack#readme",
  "xpack": {
    "minimumXpmRequired": "0.19.1",
    "binaries": {
      "destination": "./.content",
      "rootUrl": "https://github.com/mmomtchev/swig-xpack/releases/download",
      "skip": 1,
      "platforms": {
        "darwin-arm64": {
          "fileName": "swig-xpack-5.0.11-1-darwin-arm64.tar.gz",
          "sha256": "34a2000af575a45f917ac56f568995d9ceffcffd96d6935f5a14fab74e58bee8"
        },
        "darwin-x64": {
          "fileName": "swig-xpack-5.0.11-1-darwin-x64.tar.gz",
          "sha256": "ced6e732a98c37d89a4cadc040a3ea9a669c542d2f49e48bbc79b3dac9ab901e"
        },
        "linux-x64": {
          "fileName": "swig-xpack-5.0.11-1-linux-x64.tar.gz",
          "sha256": "3c9b3b3b63f2bb20ef3cb4a91dddd1fe2f85a05aab7a593f11d20bb2fdc15e7c"
        },
        "win32-x64": {
          "fileName": "swig-xpack-5.0.11-1-win32-x64.zip",
          "sha256": "525bf7e19438aadbc72a7f360371fb7aa11bde09de7d18808e13b5d064d3323c"
        },
        "linux-arm64": {
          "fileName": "swig-xpack-5.0.11-1-linux-arm64.tar.gz",
          "sha256": "46e34b48124652e575772bbb15686fae6a28a085ef82445cfca32fd16c88e442"
        },
        "win32-arm64": {
          "fileName": "swig-xpack-5.0.11-1-win32-arm64.zip",
          "sha256": "7507f27ef81eaf82b68e8143af69bc14172e48783cc32f241f22771725dd2b49"
        }
      },
      "baseUrl": "https://github.com/mmomtchev/swig-xpack/releases/download/v5.0.11-1"
    },
    "bin": {
      "swig": "./.content/swig-jse",
      "swig-jse": "./.content/swig-jse"
    },
    "properties": {
      "version": "5.0.11"
    },
    "actions": {
      "install": "conan install -of tmp --requires swig-jse/{{ properties.version }} --deployer-folder=content --deployer-package='*' --deployer=runtime_deploy",
      "test": [
        ".{{ path.sep }}content{{ path.sep }}swig-jse -version",
        ".{{ path.sep }}content{{ path.sep }}swig-jse -swiglib",
        ".{{ path.sep }}content{{ path.sep }}swig-jse -javascript -c++ -napi test/example.i"
      ]
    }
  },
  "dependencies": {
    "xpm": "^0.19.2"
  }
}