{
  "name": "@mmomtchev/swig-xpack",
  "version": "5.0.10-2",
  "description": "SWIG JSE xPack",
  "scripts": {
    "postversion": "git push && git push --tags && node ./publish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmomtchev/swig-xpack.git"
  },
  "keywords": [
    "swig",
    "swig-jse",
    "wasm",
    "node-api",
    "xpack"
  ],
  "author": "Momtchil Momtchev <momtchil@momtchev.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmomtchev/swig-xpack/issues"
  },
  "homepage": "https://github.com/mmomtchev/swig-xpack#readme",
  "xpack": {
    "minimumXpmRequired": "0.19.1",
    "binaries": {
      "destination": "./.content",
      "rootUrl": "https://github.com/mmomtchev/swig-xpack/releases/download",
      "skip": 1,
      "platforms": {
        "darwin-arm64": {
          "fileName": "swig-xpack-5.0.10-2-darwin-arm64.tar.gz",
          "sha256": "42efb548fdf594cc4945411f0a129ecc6011b994c5f3cc5b6fa34fb9859be62d"
        },
        "darwin-x64": {
          "fileName": "swig-xpack-5.0.10-2-darwin-x64.tar.gz",
          "sha256": "3df63a01510a069a8a6701dd8190aebd44317467c97aae6e3297f87f04fea967"
        },
        "linux-x64": {
          "fileName": "swig-xpack-5.0.10-2-linux-x64.tar.gz",
          "sha256": "b61ebe970c4103ac2889371c85d839ffc5b3c63fccf4cc20666b0f2840a797ba"
        },
        "win32-x64": {
          "fileName": "swig-xpack-5.0.10-2-win32-x64.zip",
          "sha256": "2204f20f6d1cc9af7d205caa33faa9163f6feedf863755f4f92e8af112769478"
        }
      },
      "baseUrl": "https://github.com/mmomtchev/swig-xpack/releases/download/v5.0.10-2"
    },
    "bin": {
      "swig": "./.content/swig-jse",
      "swig-jse": "./.content/swig-jse"
    },
    "properties": {
      "version": "5.0.10"
    },
    "actions": {
      "install": "conan install -of tmp --requires swig-jse/{{ properties.version }} --deployer-folder=content --deployer-package='*' --deployer=runtime_deploy",
      "test": [
        ".{{ path.sep }}content{{ path.sep }}swig-jse -version",
        ".{{ path.sep }}content{{ path.sep }}swig-jse -swiglib"
      ]
    }
  },
  "dependencies": {
    "xpm": "^0.19.2"
  }
}