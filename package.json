{
  "name": "@mmomtchev/swig-xpack",
  "version": "5.0.10-3",
  "description": "SWIG JSE xPack",
  "scripts": {
    "postversion": "git push && git push --tags && node ./publish.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmomtchev/swig-xpack.git"
  },
  "keywords": [
    "swig",
    "swig-jse",
    "wasm",
    "node-api",
    "xpack"
  ],
  "author": "Momtchil Momtchev <momtchil@momtchev.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmomtchev/swig-xpack/issues"
  },
  "homepage": "https://github.com/mmomtchev/swig-xpack#readme",
  "xpack": {
    "minimumXpmRequired": "0.19.1",
    "binaries": {
      "destination": "./.content",
      "rootUrl": "https://github.com/mmomtchev/swig-xpack/releases/download",
      "skip": 1,
      "platforms": {
        "darwin-arm64": {
          "fileName": "swig-xpack-5.0.10-3-darwin-arm64.tar.gz",
          "sha256": "6212dc6bbb42372d6008fd735ce2e9172768b54e7d630ff6c0e3f11453a834f2"
        },
        "darwin-x64": {
          "fileName": "swig-xpack-5.0.10-3-darwin-x64.tar.gz",
          "sha256": "e7f37c5047bbad3a4d24ed5063597a5ac1c3f7adbf32d732bb56f46d95c80149"
        },
        "linux-x64": {
          "fileName": "swig-xpack-5.0.10-3-linux-x64.tar.gz",
          "sha256": "86ca906a8eb54fe2d89c9ca7e0302fb53af5b23506bbef20a9510c4d4b633257"
        },
        "win32-x64": {
          "fileName": "swig-xpack-5.0.10-3-win32-x64.zip",
          "sha256": "7f5fdb3239a09275dcbf6a0aa2d436c6e658dcfabf08f79c4ce13198e8e56e15"
        },
        "linux-arm64": {
          "fileName": "swig-xpack-5.0.10-3-linux-arm64.tar.gz",
          "sha256": "ef8f471b3d6c131740b2043b4491b23f96ee34a288130d7dc5377caede84feb7"
        },
        "win32-arm64": {
          "fileName": "swig-xpack-5.0.10-3-win32-arm64.zip",
          "sha256": "8aaa90f55e8ee90ac34b2f6c71ae40247838a194df11806d3e8c4e34dcdefcdb"
        }
      },
      "baseUrl": "https://github.com/mmomtchev/swig-xpack/releases/download/v5.0.10-3"
    },
    "bin": {
      "swig": "./.content/swig-jse",
      "swig-jse": "./.content/swig-jse"
    },
    "properties": {
      "version": "5.0.11"
    },
    "actions": {
      "install": "conan install -of tmp --requires swig-jse/{{ properties.version }} --deployer-folder=content --deployer-package='*' --deployer=runtime_deploy",
      "test": [
        ".{{ path.sep }}content{{ path.sep }}swig-jse -version",
        ".{{ path.sep }}content{{ path.sep }}swig-jse -swiglib",
        ".{{ path.sep }}content{{ path.sep }}swig-jse -javascript -c++ -napi test/example.i"
      ]
    }
  },
  "dependencies": {
    "xpm": "^0.19.2"
  }
}
